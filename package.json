{
  "name": "time-a-productivity-streak-builder",
  "displayName": "Time: A Streak & productivity Habit Builder",
  "description": "Local-first coding time tracker with streaks and rewards",
  "publisher": "BuddyTools",
  "icon": "media/icon.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "time-a-productivity-streak-builder.openStopwatch",
        "title": "Time Streak Builder: Open Stopwatch"
      },
      {
        "command": "time-a-productivity-streak-builder.markProjectComplete",
        "title": "Time Streak Builder: Mark Project Complete"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.107.0",
    "typescript": "^5.4.0"
  }
}
